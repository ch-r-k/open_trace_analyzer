{
    "input_file": "trace.txt",
    "input_file_type": "txt",
    "output_file": "out.puml",
    "output_file_type": "puml_sequence",
    "task_object_file": "tasks.json",
    "txt_config": {
        "task_switch": {
            "regex": "^\\s*(\\d+)\\s+(Sch-(Next|Idle))\\s+Pri=(\\d+)->(\\d+)",
            "groups": [
                {
                    "index": 1,
                    "name": "timestamp",
                    "description": "timestamp"
                },
                {
                    "index": 2,
                    "name": "schedule_status",
                    "description": "Full schedule status, e.g., 'Sch-Next' or 'Sch-Idle'"
                },
                {
                    "index": 3,
                    "name": "status_type",
                    "description": "The type of schedule status (Next or Idle)"
                },
                {
                    "index": 4,
                    "name": "task_from",
                    "description": "Priority of task switched from"
                },
                {
                    "index": 5,
                    "name": "task_to",
                    "description": "Priority of task switched to"
                }
            ]
        },
        "state_machine": {
            "regex": "^\\s*(\\d+)\\s+===\\>Tran\\s+Obj=([^,]+),.*State=[^>]+->([^,]+)",
            "groups": [
                {
                    "index": 1,
                    "name": "timestamp",
                    "description": "Timestamp"
                },
                {
                    "index": 2,
                    "name": "task",
                    "description": "Task to switch state"
                },
                {
                    "index": 3,
                    "name": "state_name",
                    "description": "State name of new state"
                }
            ]
        },
        "event_message": {
            "regex": "^\\s*(\\d+).*AO-Post.*Sdr=([^,]+),Obj=([^,]+).*Sig=([^,>]+)",
            "groups": [
                {
                    "index": 1,
                    "name": "timestamp",
                    "description": "timestamp"
                },
                {
                    "index": 2,
                    "name": "task_from",
                    "description": "Task which sends the message"
                },
                {
                    "index": 3,
                    "name": "task_to",
                    "description": "Task which receives the message"
                },
                {
                    "index": 4,
                    "name": "message_text",
                    "description": "Message Name"
                }
            ]
        },
        "note": {
            "regex": "^(\\d{10})\\s+(.*)$",
            "groups": [
                {
                    "index": 1,
                    "name": "timestamp",
                    "description": "timestamp"
                },
                {
                    "index": 2,
                    "name": "text",
                    "description": "Note Text"
                }
            ]
        }
    }
}